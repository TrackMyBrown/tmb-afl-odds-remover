{
    "manifest_version": 3,
    "name": "TrackMyBrown AFL Odds Remover",
    "version": "1.0",
    "description": "Removes gambling odds from AFL ladder and fixture pages",
    "content_scripts": [
      {
        "matches": [
          "https://www.afl.com.au/ladder*",
          "https://www.afl.com.au/fixture*"
        ],
        "js": ["content.js"]
      }
    ],
    "icons": {
    "16": "tmb-ico-16.png",
    "48": "tmb-ico-48.png",
    "128": "tmb-ico-128.png"
  }
  }
  